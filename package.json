{
  "name": "san-store",
  "version": "2.1.3",
  "description": "Application State Management for San",
  "scripts": {
    "build": "rm -rf dist && rollup --config && uglifyjs dist/san-store.source.js -mco dist/san-store.js && uglifyjs dist/san-store-use.source.js -mco dist/san-store-use.js && cp types/*.d.ts dist",
    "pretest": "npm run build && webpack --config test/webpack.config.js",
    "test-server": "npm run build && webpack-dev-server --config test/webpack.config.js",
    "test": "npm run build && karma start test/karma.config.js",
    "lint": "eslint --ignore-path .eslintignore ./"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/preset-env": "^7.7.7",
    "@ecomfe/eslint-config": "^7.3.0",
    "@rollup/plugin-replace": "^3.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^7.18.0",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.0",
    "karma-webpack": "^2.0.1",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "san": "^3.11.0",
    "san-composition": "^1.3.0",
    "san-update": "2.0.0",
    "uglify-js": "^2.7.5",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
 
  "types": "index.d.ts",
  "typesVersions": {
    ">=4.2": {
      "*": [
        "dist/*"
      ]
    }
  },

  "main": "dist/san-store.source.js",
  "unpkg": "dist/san-store",
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "peerDependencies": {
    "san-composition": "^1.3.0",
    "san-update": "2.x"
  },
  "browserslist": [
    "IE 8"
  ],
  "author": "errorrik",
  "repository": {
    "type": "git",
    "url": "https://github.com/baidu/san-store.git"
  },
  "license": "MIT"
}
